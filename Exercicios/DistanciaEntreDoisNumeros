package DesafioProgramandoTodoDia;

import javax.swing.*;

public class DistanciaEntreDoisPontos {

    public static void main(String[] args) {

        float y1;
        float x1;
        float y2;
        float x2;
        float result;

        x1 = Float.parseFloat(JOptionPane.showInputDialog("Digite o primeiro número"));
        y1 = Float.parseFloat(JOptionPane.showInputDialog("Digite o segundo número"));

        x2 = Float.parseFloat(JOptionPane.showInputDialog("Digite o terceiro número"));
        y2 = Float.parseFloat(JOptionPane.showInputDialog("Digite o quarto número"));

        result = (float) Math.sqrt(Math.pow((x2 - x1), 2) + Math.pow((y2 - y2), 2));
        JOptionPane.showMessageDialog(null, result);
    }
}
